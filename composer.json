{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "atayahmet/laravel-nestable": "^0.8.9",
        "barryvdh/laravel-cors": "^0.11.3",
        "brozot/laravel-fcm": "^1.2",
        "bugsnag/bugsnag-laravel": "^2.15",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "3.3",
        "laravel/framework": "5.8.*",
        "laravel/passport": "^7.3",
        "laravel/tinker": "^1.0",
        "maatwebsite/excel": "^3.1",
        "mashape/unirest-php": "^3.0",
        "predis/predis": "^1.1",
        "rap2hpoutre/fast-excel": "^1.3",
        "realrashid/sweet-alert": "^2.0",
        "spatie/laravel-permission": "^2.37",
        "steevenz/rajaongkir": "^2.2",
        "uxweb/sweet-alert": "^2.0",
        "veritrans/veritrans-php": "^1.2",
        "xendit/xendit-php": "^2.10",
        "yajra/laravel-datatables-oracle": "^9.4"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/dusk": "^5.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
